  <ItemGroup>
    <PackageReference Include="bHapticsLib" Version="1.0.6" />
    <PackageReference Include="BuildSoft.OscCore" Version="1.2.1.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
